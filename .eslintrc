{
  "parser": "babel-eslint",
  "extends": ["standard", "plugin:flowtype/recommended"],
  "plugins": ["flowtype"],
  "rules": {
    "standard/no-callback-literal": "off",
    "flowtype/no-types-missing-file-annotation": "off"
  },
  "settings": {
    "flowtype": {
      "onlyFilesWithFlowAnnotation": true
    }
  }
}